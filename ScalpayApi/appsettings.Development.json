{
  "urls": "http://*:5000",
  "Database": {
    "Type": "sqlite",
    "ConnectionStrings": "Data Source=sqlite.db;"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Debug",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  }
}
